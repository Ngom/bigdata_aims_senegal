    "import time as time\n",
    "from datetime import datetime\n",
    "def unix_time(dt):\n",
    "    epoch = datetime.utcfromtimestamp(0)\n",
    "    delta = dt - epoch\n",
    "    return delta.total_seconds()\n",
    "\n",
    "def unix_time_millis(dt):\n",
    "    return unix_time(dt) * 1000.0"
   ]
  },
